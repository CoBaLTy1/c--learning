using System;

namespace first
    {
    class program
    {
        static void Main(string[] args)
        {

            int x = int.Parse(Console.ReadLine());
            int y = int.Parse(Console.ReadLine());
            int z = x + y;
            if (x > 0 && y > 0) { 
                Console.WriteLine(z);
            }

            Console.ReadKey();
        }
    }
}
